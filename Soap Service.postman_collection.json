	"item": [
		{
			"name": "NumberToWordConvert",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const xml2js = require('xml2js');\r",
							"const parser = new xml2js.Parser();\r",
							"\r",
							"const xmlData = pm.response.text();\r",
							"\r",
							"parser.parseString(xmlData, function (err, result) {\r",
							"    if (err) {\r",
							"        console.log(\"Error parsing XML:\", err);\r",
							"    } else {\r",
							"        const responseJson = result;\r",
							"        console.log(responseJson);\r",
							"\r",
							"        pm.test(\"Validate String of response\", function() {\r",
							"            const actualValue = responseJson[\"soap:Envelope\"][\"soap:Body\"][0][\"m:NumberToWordsResponse\"][0][\"m:NumberToWordsResult\"][0];\r",
							"            pm.expect(pm.collectionVariables.get(\"result\")).to.eql(actualValue);\r",
							"        });\r",
							"    }\r",
							"});\r",
							"pm.test(\"parsing the response\",function()\r",
							"{\r",
							"\r",
							"   pm.response.to.have.status(200);\r",
							"   pm.expect(pm.response.text()).to.include(pm.collectionVariables.get(\"result\"));\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
